{
  "entrypoint": "index.html",
  "shell": "src/components/app-shell.js",
  "sources": ["data/**/*", "images/**/*", "locales/**/*", "videos/**/*"],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/*.{js,js.map}",
    "node_modules/@webcomponents/webcomponentsjs/bundles/**",
    "manifest.json",
    "push-manifest.json"
  ],
  "builds": [
    {
      "preset": "es5-bundled",
      "js": { "compile": true },
      "bundle": false,
      "basePath": "/"
    }
  ]
}
