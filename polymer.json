{
  "entrypoint": "index.html",
  "shell": "src/app-shell.html",
  "fragments": [
    "src/pages/page-home.html",
    "src/pages/page-acqua.html",
    "src/pages/page-not-found.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    ".nojekyll",
    "CNAME",
    "manifest.json",
    "sitemap.xml"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "addServiceWorker": false
    }
  ]
}
