{
  "extends": ["airbnb-base", "prettier"],

  "parser": "babel-eslint",

  "parserOptions": {
    "sourceType": "module"
  },

  "env": {
    "browser": true,
    "es6": true
  },

  "rules": {
    "import/extensions": "off",
    "import/prefer-default-export": "off",

    "class-methods-use-this": "off"
  }
}
